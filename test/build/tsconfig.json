
        {
            "include": ["src"],
            "compilerOptions": {
            "module": "esnext",
            "target": "esnext",
            "moduleResolution": "node",
            "jsx": "preserve",
            "baseUrl": "./",
            "noEmit": true,
            /* Additional Options */
            "strict": true,
            "skipLibCheck": true,
            "forceConsistentCasingInFileNames": true,
            "resolveJsonModule": true,
            "useDefineForClassFields": true,
            "allowSyntheticDefaultImports": true,
            "importsNotUsedAsValues": "error"
            }
        }
      